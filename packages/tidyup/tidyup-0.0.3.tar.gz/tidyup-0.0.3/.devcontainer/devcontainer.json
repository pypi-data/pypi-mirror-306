{
    "name": "Tidyup",
    "dockerFile": "Dockerfile",
    "context": "..",
    "settings": {
        "terminal.integrated.shell.linux": "/bin/bash"
    },
    "extensions": [
        "ms-python.python",
        "ms-vscode-remote.remote-containers"
    ],
    "remoteUser": "root",
    "mounts": [
        "source=/,target=/mnt,type=bind"
    ]
    
}