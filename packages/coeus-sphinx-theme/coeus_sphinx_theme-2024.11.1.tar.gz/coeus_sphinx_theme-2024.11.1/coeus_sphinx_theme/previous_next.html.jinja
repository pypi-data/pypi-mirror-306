<div class="previous-next">
    <div id="previous-button" {% if not prev %}style="opacity: 0.3;" {% endif %}>
        <a href="{{ prev.link|e }}">
            <span><i class="fa fa-chevron-left" style="font-size: 1.5rem;"></i></span>
        </a>
    </div>
    <div id="next-button" {% if not next %}style="opacity: 0.3;" {% endif %}>
        <a href="{{ next.link|e }}">
            <span><i class="fa fa-chevron-right" style="font-size: 1.5rem;"></i></span>
        </a>
    </div>
</div>
